    "path": "^0.12.7",
    "simple-peer": "^9.9.3",
    "socket.io": "^2.3.0",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.8",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/simple-peer": "^9.6.1",
