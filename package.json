    "@types/socket.io": "^2.1.11",
    "@types/uuid": "^8.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "is-subset-of": "^3.1.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongodb": "^3.5.9",
